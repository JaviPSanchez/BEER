{
  "name": "beer",
  "version": "1.0.0",
  "description": "pagina ejemplo",
  "default": "index.html",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist"
  },
  "author": "Javier Palomino Sanchez",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.2",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "sass": "^1.35.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaviPSanchez/BEER.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/JaviPSanchez/BEER/issues"
  },
  "homepage": "https://github.com/JaviPSanchez/BEER#readme"
}
